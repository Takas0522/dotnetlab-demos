{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@takas0522/utility": {
      "projectType": "library",
      "root": "projects/takas0522/utility",
      "sourceRoot": "projects/takas0522/utility/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/takas0522/utility/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/takas0522/utility/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/takas0522/utility/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/takas0522/utility/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/takas0522/utility/**/*.ts",
              "projects/takas0522/utility/**/*.html"
